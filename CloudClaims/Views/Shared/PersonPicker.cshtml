@model CloudClaims.ViewModels.vmPersonPicker

<!-- Main content wrapper -->
<div class="">
    <!-- Dynamic table -->
    <div class="widget">
        <div class="title">
            <img src="~/Content/crwn/images/icons/dark/users.png" alt="" class="titleIcon" />
            @*<h6>Person Search</h6>*@
        </div>
        <table cellpadding="0" cellspacing="0" border="0" class="display dTable">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var p in Model.person) {
                    <tr id="personID-@p.personID" class="gradeA draggable-claim-personnel cursor-pointer">
                        <td>@p.personFName</td>
                        <td>@p.personLName oswifjosifjosei @Model.mileage</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
